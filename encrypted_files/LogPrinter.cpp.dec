#include "Log.h"

void LogEntry::print() {
   std::cout << timestamp << '|' << category << '|' << message << '\n';
}
void Log::print() {
   for(auto &le: entries) {
      le.print();
   }
   YELL(entries.size() << " entries printed.\n");
}
void Log::print(std::vector<int> indices) {
   int c=0;
   for(auto i: indices) {
      std::cout << i << "|"; entries[i].print();
      ++c;
   }
   YELL(c << " entries printed.\n");
}
